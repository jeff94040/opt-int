            <td><%=op3.age%></td>
            <td></td>
          </tr>
          <tr data-menu="3">
            <td>output</td>
            <td>testit.op4</td>
            <td>condensed open summary (unit 7)</td>
            <td><a href='/view/testit.op4'>view</a></td>
            <td><a href='/download/testit.op4'>download</a></td>
            <td><%=op4.age%></td>
            <td></td>
          </tr>
          <tr data-menu="1">
            <td>output</td>
            <td>testit.wav</td>
            <td>wave-data output (unit 31)</td>
            <td><a href='/view/testit.wav'>view</a></td>
            <td><a href='/download/testit.wav' download="">download</a></td>
            <td><%=wav.age%></td>
            <td></td>
          </tr>
          <tr data-menu="1">
            <td>output</td>
            <td>testit.wav.ssv</td>
            <td>testit.wav with space separated values</td>
            <td></td>
            <td><a href='/download/testit.wav.ssv' download="">download</a></td>
            <td><%=wav.age%></td>
            <td></td>
          </tr>   
          <tr data-menu="3">
            <td>output</td>
            <td>testit.pl1</td>
            <td>plot data 1</td>
