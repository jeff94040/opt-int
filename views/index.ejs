<body class="bg-light">

  <!-- Bootstrapped main menu -->
  <div class="container-fluid mt-3">
    <div class="d-inline-flex">
      <select class="form-select" id="menu">
        <option value="0">Select a process to run:</option>
        <option value="1">Kelvin Wake</option>
        <option value="2">Hull Optimization</option>
        <option value="3">Resistance Predictions</option>
      </select>
    </div>
  </div>

