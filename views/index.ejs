            <td><a href='/download/testit.shp'>download</a></td>
            <td><%=shp.age%></td>
            <td><input class="form-control form-control-sm" type='file' name='shp' accept='.shp'></td>
          </tr>
          <tr data-menu="1,2">
            <td>input</td>
            <td>testit.stg</td>
